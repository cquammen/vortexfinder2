<ServerManagerConfiguration>
  <ProxyGroup name="filters">
    <SourceProxy name="GLGPUVortexFilter" class="GLGPUVortexFilter" label="GLGPUVortexFilter">
      <Documentation
         short_help="extract vortex from glgpu data"
         long_help="extract vortex from glgpu data">
      </Documentation>
      <InputProperty
        name="Input"
        command="SetInputConnection">
        <ProxyGroupDomain name="groups">
          <Group name="sources"/>
          <Group name="filters"/>
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkImageData"/>
        </DataTypeDomain>
      </InputProperty>

      <Hints>
        <ShowInMenu category="Extensions"/>
      </Hints>
    </SourceProxy>
  </ProxyGroup>
</ServerManagerConfiguration>
